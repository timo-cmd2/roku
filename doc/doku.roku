# Doku, A documentation generator for 
# the Roku programmong language.
require std
require io
require string

module doc.doku

class Doku is Int
  def main(args) is Int
    var file = io.file
    
    if args = file
      writeline "generating docs..."
    end
  end
end

var doku = new.Doku
Doku.main
